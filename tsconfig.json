{
  "compilerOptions": {
    "alwaysStrict": true,
    "inlineSourceMap": true,
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "es5",
      "es2015.iterable",
      "es2015.collection",
      "es2015.promise",
      "es2015.symbol",
      "es2015.symbol.wellknown",
      "dom"
    ],
    "outDir": "./"
  },
  "include": [
    "test/**/*.ts",
    "lib/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
